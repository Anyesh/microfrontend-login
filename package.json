{
  "name": "totoro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "svgr": "svgr assets/icons -d assets/IconComponents --config-file .svgrrc.json --svgo-config .svgorc.json",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "prepare": "husky install",
    "check-lint": "eslint . --ext ts --ext tsx --ext js --quiet --fix",
    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run check-lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix -f git-log",
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}/": [
      "eslint --fix",
      "git add"
    ],
    "*.{ts,css,md}": "prettier --write"
  },
  "dependencies": {
    "@headlessui/react": "^1.2.0",
    "axios": "^0.21.1",
    "jwt-decode": "^3.1.2",
    "memory-cache": "^0.2.0",
    "next": "10.2.0",
    "next-auth": "^3.24.0",
    "next-themes": "^0.0.14",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-toast-notifications": "^2.4.4",
    "recompose": "^0.30.0",
    "swr": "^0.5.6"
  },
  "devDependencies": {
    "@svgr/cli": "^5.5.0",
    "@types/cookie": "^0.4.0",
    "@types/jwt-decode": "^3.1.0",
    "@types/memory-cache": "^0.2.1",
    "@types/node": "^15.0.1",
    "@types/react": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "autoprefixer": "^10.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.27.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "postcss": "^8.2.13",
    "prettier": "^2.2.1",
    "tailwind-nord": "^1.2.0",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.3.2"
  }
}
